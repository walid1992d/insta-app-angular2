{
  "name": "insta-app-angular2",
  "version": "0.0.1",
  "author": "Walid Abou Ali",
  "homepage": "https://github.ibm.com/Walid-Abou-Ali",
  "description": "Boilerplate for ionic with electron",
  "main": "src/electron-starter.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c=local",
    "build": "ng build -c=local ",
    "build-local": "ng build -c=local ",
    "build-dev": "ng build -c=dev ",
    "build-prod": "ng build -c=production ",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:android": "ionic cordova run android --device --configuration local",
    "start:ios": "ionic cordova run ios --device --configuration local",
    "start:desktop": "npm run build-local  && electron . ",
    "build:browser:dev": "npm run build-dev",
    "build:browser:prod": "npm run build-prod ",
    "build:browser:local": "npm run build-local",
    "build:ios:dev": " ionic cordova build --configuration dev",
    "build:ios:prod": "ionic cordova build --configuration production",
    "build:ios:local": "ionic cordova build ios --configuration local",
    "build:android:dev": "ionic cordova build android --configuration dev",
    "build:android:prod": "ionic cordova build android  --configuration production",
    "build:android:local": "ionic cordova build --configuration local",
    "electron": "electron .",
    "build:osx:local": "npm run build:browser:local && node electron-scripts/osx-build.js",
    "build:osx:dev": "npm run build:browser:dev && node electron-scripts/osx-build.js",
    "build:osx:prod": "npm run build:browser:prod && node electron-scripts/osx-build.js",
    "build:windows:local": "npm run build:browser:local node electron-scripts/windows-build.js",
    "build:windows:dev": "npm run build:browser:dev node electron-scripts/windows-build.js",
    "build:windows:prod": "npm run build:browser:prod node electron-scripts/windows-build.js",
    "build:linux:local": "npm run build:browser:local node electron-scripts/linux-build.js",
    "build:linux:dev": "npm run build:browser:dev node electron-scripts/linux-build.js",
    "build:linux:prod": "npm run build:browser:prod node electron-scripts/linux-build.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/pwa": "^0.803.22",
    "@angular/router": "~8.1.2",
    "@angular/service-worker": "~8.1.2",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^4.7.1",
    "cordova-android": "7.1.4",
    "cordova-ios": "^5.1.1",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^4.1.3",
    "cordova-plugin-splashscreen": "^5.0.3",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.1",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.801.2",
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular-devkit/core": "~8.1.2",
    "@angular-devkit/schematics": "~8.1.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@ionic/angular-toolkit": "^2.1.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2",
    "electron-packager": "^13.1.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "minify-all": "^1.2.2",
    "minimist": "^1.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}